<div id="accounts-search-dialog" title="Kfs Search Account" style="display:none;">
    <form id="accounts-search-form">
        <section class="section-contents">
            <div class="section-text">
                <p>
                    Enter an account name or ID and then hit enter (or click the <img src="@Url.Image("details.png")" /> icon).
                    Matching accounts from the campus financial system will be displayed. Finally, click 'Select' next to your desired account.
                    <br/>
                    <strong>NOTE!</strong> Selecting an account from this list instead of the list available in the order will route this order to the account manager associated with the selected account and may bypass the approver.
                </p>    
            </div>
            <div>
                <input type="hidden" id="accounts-search-dialog-selected"/>
                <input type="text" id="accounts-search-dialog-searchbox" placeholder="Acct # or Name"/>
                <a href="#" id="accounts-search-dialog-searchbox-btn"><img src="@Url.Image("details.png")" /></a>
                <table id="accounts-search-dialog-results">
                    <thead>
                        <tr>
                            <th></th>
                            <th>Name</th>
                            <th>Account</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </section>
    </form>
</div>