.roundedBox{width:100%;margin:20px auto 0 auto;background:#fff url(/Content/Img/Common/rb_leftside.gif) repeat-y left top;}.roundedBoxRed{width:100%;margin:0 auto;background:#fff url(/Content/Img/Common/rb_leftside_red.gif) repeat-y left top;}.roundedBoxTop{width:100%;height:20px;background:url(/Content/Img/Common/rb_top.gif) no-repeat left top;}.roundedBoxTopRed{width:100%;height:20px;background:url(/Content/Img/Common/rb_top_red.gif) no-repeat left top;}.roundedBoxTop span{display:block;position:relative;height:20px;background:url(/Content/Img/Common/rb_topright.gif) no-repeat right top;}.roundedBoxTopRed span{display:block;position:relative;height:20px;background:url(/Content/Img/Common/rb_topright_red.gif) no-repeat right top;}.roundedBoxContent{position:relative;background:url(/Content/Img/Common/rb_rightside.gif) repeat-y right top;min-height:100px;padding:4px 20px 10px 25px;margin:-1px 0 -10px 0;}.roundedBoxContentRed{position:relative;background:url(/Content/Img/Common/rb_rightside_red.gif) repeat-y right top;min-height:100px;padding:8px 20px 10px 25px;margin:-1px 0 -10px 0;}.roundedBoxBottom{width:100%;height:40px;background:url(/Content/Img/Common/rb_bottom.gif) no-repeat left bottom;}.roundedBoxBottomRed{width:100%;height:40px;background:url(/Content/Img/Common/rb_bottom_red.gif) no-repeat left bottom;}.roundedBoxBottom span{display:block;position:relative;height:40px;background:url(/Content/Img/Common/rb_bottomright.gif) no-repeat right top;}.roundedBoxBottomRed span{display:block;position:relative;height:40px;background:url(/Content/Img/Common/rb_bottomright_red.gif) no-repeat right top;}#centerwrap div.roundedBoxContent .topRight{float:right;display:block;position:relative;top:-3px;right:0;margin-left:10px;}#centerwrap div.roundedBoxContent .topRight a:link{text-decoration:none;}#centerwrap div.roundedBoxContent a:link{text-decoration:underline;}#centerwrap div.roundedBoxContent a:hover{text-decoration:none;}#centerwrap div.roundedBoxContent p{line-height:1.3em;font-size:11px;}#centerwrap div.roundedBoxContent p.detail-date-needed{padding:0;margin-bottom:15px;font-weight:normal;font-size:15px;}#centerwrap div.roundedBoxContent fieldset.first{-moz-border-radius:10px 10px 0 0;-webkit-border-radius:10px 10px 0 0;border-radius:10px 10px 0 0;}#centerwrap div.roundedBoxContent fieldset.last{-moz-border-radius:0 0 10px 10px;-webkit-border-radius:0 0 10px 10px;border-radius:0 0 10px 10px;}#centerwrap div.roundedBoxContent h1{padding:0;margin-bottom:3px;}div.details-col-wrap{clear:both;}div.details-col{float:left;width:33.33%;}div.details-col fieldset{margin:0 8px;min-height:130px;}div.details-col fieldset#col-3{margin-right:0;}div.details-col fieldset#col-1{margin-left:0;}#centerwrap div.details-col legend span a:link{text-decoration:none;border:0 none;margin-left:5px;}#centerwrap div.details-col p{margin-bottom:0;}#centerwrap div.details-col ul{list-style-type:disc;padding-left:1em;margin-left:3px;}#centerwrap div.details-col ul li{padding:3px 0;font-size:10px;}#centerwrap div.details-col ul ul{margin:5px 0 0 0;list-style-type:none;}#centerwrap div.details-col ul ul img{float:left;margin-top:-3px;}#centerwrap div.details-col h2{padding:0;margin-bottom:8px;font-size:13px;}#centerwrap div.details-col strong{color:#656565;font-style:italic;}#centerwrap table.detailsTbl,#centerwrap table.detailsTbl th{font-size:11px;line-height:1.3em;}#centerwrap table.detailsTbl th,#centerwrap table.detailsTbl td{padding:3px 4px;border-bottom:solid 1px #CCC;}#centerwrap table.detailsTbl tr.alt td{background-color:#FEFEFE;}#centerwrap table.detailsTbl tr.last td{border-bottom:0 none;}.external-img{position:relative;overflow:visible;width:16px;}.external-img img{position:absolute;top:-1px;vertical-align:middle;}#centerwrap #add-comments{float:right;margin-top:-10px;}#centerwrap ul.comments-list{margin-top:15px;}#centerwrap ul.comments-list li{padding:8px 10px;font-size:10px;background:#FFF url(/Content/Img/Common/comment.png) no-repeat right top;border-top:1px solid #DDD;border-left:1px solid #DDD;border-right:1px solid #DDD;}#centerwrap ul.comments-list li.last{-moz-border-radius:0 0 10px 10px;border-bottom:1px solid #DDD;-webkit-border-radius:0 0 10px 10px;border-radius:0 0 10px 10px;}#centerwrap ul.comments-list li.owner{background:#FFF;}#centerwrap ul.comments-list li p{margin-bottom:8px;font-size:10px;}#centerwrap ul.comments-list div.comment-date{font-style:italic;margin-bottom:5px;}#centerwrap ul.comments-list ul{margin:0 0 8px 2em;}#centerwrap ul.comments-list ul li{border:0 none;padding:0;line-height:16px;background:none;}#centerwrap ul.comments-list ul li a{padding-left:16px;background:#FFF url(/Content/Img/Fugue/paper-clip-small.png) no-repeat left center;}